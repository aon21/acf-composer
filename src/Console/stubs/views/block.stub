<div class="{{ $block->classes }}"
  @if (!empty($block->inlineStyle))
    style="{{ $block->inlineStyle }}"
  @endif
  @if (!empty($block->anchor))
    id="{{ $block->anchor }}"
  @endif
>
  @if ($items)
    <ul>
      @foreach ($items as $item)
        <li>{{ $item['item'] }}</li>
